{
  "build": {
    "beforeBuildCommand": "bun run astro:build",
    "beforeDevCommand": "bun run astro:dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:4321"
  },
  "bundle": {
    "active": true,
    "category": "Game",
    "copyright": "2024 TeaClient",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "teaclient",
  "version": "0.1.0",
  "identifier": "net.teaclient",
  "plugins": {},
  "app": {
    "macOSPrivateApi": true,
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "width": 1715,
        "height": 1085,
        "minWidth": 1329,
        "minHeight": 729,
        "resizable": true,
        "title": "Teaclient",
        "decorations": false,
        "transparent": true,
        "shadow": true
      }
    ],
    "security": {
      "freezePrototype": true,
      "csp": {
        "img-src": "'self'",
        "connect-src": [
          "ipc: http://ipc.localhost"
        ],
        "default-src": "'self'"
      }
    }
  }
}